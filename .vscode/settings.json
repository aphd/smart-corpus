{
    "files.exclude": {
        "node_modules": true,
        "docs": true,
        "public": true,
        ".pnp": true,
        "**/.pnp.js": true,
        "coverage": true,
        "build": true,
        "**/.DS_Store": true,
        "**/.env.local": true,
        "**/.env.development.local": true,
        "**/.env.test.local": true,
        "**/.env.production.local": true,
        "**/npm-debug.log*": true,
        "**/yarn-debug.log*": true,
        "**/yarn-error.log*": true
    },
    "prettier": {
        "printWidth": 120,
        "semi": true,
        "singleQuote": true,
        "tabWidth": 4,
    },
    "workbench.colorCustomizations": {
        "activityBar.background": "#5F0525",
        "titleBar.activeBackground": "#850733",
        "titleBar.activeForeground": "#FFFBFD"
    }
}
